//takes a while to execute, but should work if the
//generated DISM code manages memory correctly
class C extends Object { 
  nat n;
  nat foo(nat unused) {this.n = 8;}
  nat bar(nat unused) {this.foo(7);}
}
main {
  nat i;
  C c;
  c=new C();
  i=0;
  while(!(66000<i)) {
    c.bar(6);
    i=i+1;
  };
  printNat(c.n);
}

