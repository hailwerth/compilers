mov 7 65535  ;sets FP

#INITMAIN: mov 0 0
str 6 0 0  ;sets main var 0 to zero
mov 1 1       ;DECSP-START
sub 6 6 1     ;decsp-sub
blt 6 5 #END  ;decsp-memcheck
beq 6 5 #END  ;DECSP-END

str 6 0 0  ;sets main var 1 to zero
mov 1 1       ;DECSP-START
sub 6 6 1     ;decsp-sub
blt 6 5 #END  ;decsp-memcheck
beq 6 5 #END  ;DECSP-END

#MAINEXPRS: mov 0 0
mov 0 0  ;NEW-START
str 6 0 0  ;new-push-0
mov 1 1       ;DECSP-START
sub 6 6 1     ;decsp-sub
blt 6 5 #END  ;decsp-memcheck
beq 6 5 #END  ;DECSP-END

mov 0 0  ;NEW-END
mov 0 0    ;ASSIGN-START
mov 1 65534    ;assign-dest-address
lod 2 6 1  ;assign-expr-load
str 1 0 2  ;ASSIGN-END

mov 1 1    ;INCSP-START
add 6 6 1  ;INCSP-END

